// app global css
.fs20
  font-size 20px

.fs25
  font-size 25px

.fs28
  font-size 28px

.fs30
  font-size 30px

.fs32
  font-size 32px

.panel-header
  background-color white
  border-bottom (1px solid rgba(0, 0, 0, 0.12))

.page-title
  color #444444

.border-panel
  border (1px solid rgba(0, 0, 0, 0.12))
  box-shadow (0px 1px 2px rgba(0, 0, 0, 0.1))
  border-radius 3px

.pull-right
  text-align right
  
.pull-left
  text-align left

table
  tbody
    td
      .q-btn
        padding 4px 8px

@font-face {
font-family: 'Nunito';
src: url(./Nunito-Regular.ttf);
}
body {
font-family: 'Nunito';
}

.q-table th{
 position: -webkit-sticky;
   position: sticky;
   top: 0px;
   background: #1F5EAC!important;
   z-index: 1;
   opacity 0s
   opacity: 1 !important;
 font-size: 0.95em;
}

.q-table thead, #newstyle .q-table th {
 color: #FFFFFF;
